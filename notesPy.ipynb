     "text": [
      "a = [3, 2, 1]\n",
      "b = [1, 2, 3]\n",
      "c = [8, 9, 10]\n",
      "d = [5, 6, 7]\n"
     ]
    }
   ],
   "source": [
    "a=[1,2,3]\n",
    "b=[3,2,1]\n",
    "\n",
    "#swapping a and b\n",
    "tmp=a         #it empties the variable a as the value of a goes to tmp\n",
    "a=b\n",
    "b=tmp \n",
    "print(\"a =\", a)\n",
    "print(\"b =\", b)\n",
    "\n",
    "#####T R I C K#####\n",
    "c=[5,6,7]\n",
    "d=[8,9,10]\n",
    "\n",
    "#swapping\n",
    "c,d=d,c\n",
    "print(\"c =\",c)\n",
    "print(\"d =\",d)\n"
   ]
